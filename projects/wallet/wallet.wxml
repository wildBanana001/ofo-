  <view class='my-cash same'>
    <text>我的押金</text>
    <text><text style='color:#b9dd08;'>押金退款</text> ></text>
  </view>
  <view class='my-ticket same'>
    <text>我的用车券</text>
    <text>0张 ></text>
  </view>
</view>